inisstring
  start:iniss,
  insignon:iniss {
            endsignon() ;
            gss = stringcnt ;
            instring = TRUE ;
            }

  inies:iniss {
            endstring() ;
            gss = stringcnt ;
            if (gss) 
              m_error("Multiple specification of global start string") ;
            instring = TRUE ;
            }

  inisc:iniss,
  iniec:iniss,
  initc:iniss,
  inipc:iniss,
  inistc:iniss {
            if (gss) 
              m_error("Multiple specification of global start string") ;
            gss = stringcnt ;
            instring = TRUE ;
            }

  globdef:iniss,
  globdec:iniss {
            gss = stringcnt ;
            instring = TRUE ;
            }
  ;

