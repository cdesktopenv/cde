	static int
	local_child_max(void)
	{
		int	i;
		int	n;
		for (i = 3; i < 512 && (n = child()) >= 0; i++)
			if (!n) _exit(0);
		while (wait((int*)0) > 0);
		for (n = 8; n < i; n <<= 1);
		return(n);
	}
