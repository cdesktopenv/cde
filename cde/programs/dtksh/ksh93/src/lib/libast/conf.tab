	#if !_lib_remove || _lib_unlink
	#define remove		unlink
	#endif
	static int
	local_name_max(void)
	{
		int	i;
		int	n;
		int	v;
		char	name[256];
		for (i = 0; i < sizeof(name); i++)
			name[i] = 'F';
		name[8] = '.';
		name[sizeof(name) - 1] = 0;
		if ((n = creat(name, 0666)) >= 0)
		{
			close(n);
			for (v = 12; v < sizeof(name); v++)
			{
				name[v] = 0;
				if (remove(name) >= 0) break;
				name[v] = 'F';
			}
		}
		else for (v = 12; v < sizeof(name); v++)
		{
			name[v] = 0;
			if ((n = creat(name, 0666)) >= 0)
			{
				close(n);
				remove(name);
				name[v] = 'F';
			}
			else
			{
				v--;
				break;
			}
		}
		return(v);
	}
	#endif
