#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)' 'CXXDEBUGFLAGS=$(CXXDEBUGFLAGS)'

SUBDIRS = c dnd
DONES = c/DONE dnd/DONE

INCLUDES = $(TIRPCINC)

MakeSubdirs($(SUBDIRS))
ForceSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))

SubdirLibraryRule($(DONES))
